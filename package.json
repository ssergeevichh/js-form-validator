{
  "name": "@sergeevich/js-valitool",
  "private": false,
  "version": "0.0.1",
  "main": "./dist/js-validator.upd.js",
  "module": "./dist/js-validator.es.js",
  "exports": {
    ".": {
      "import": "./dist/js-validator.es.js",
      "require": "./dist/js-validator.upd.js"
