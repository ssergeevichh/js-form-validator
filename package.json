    "coverage": "vitest run --coverage",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@gitart/eslint-config-basic": "^0.2.0",
    "eslint": "^8.18.0",
    "sass": "^1.52.3",
    "vite": "^2.9.9",
    "vitest": "^0.15.1"
  }
